    <rules>
        <template id="1.2.208.184.2.10.1" name="DKMedcomPersonalIdentification" displayName="DK Medcom Personal Identification" effectiveDate="2016-10-24T00:00:00" isClosed="false" versionLabel="V1.1" statusCode="active">
            <desc language="en-US">The Danish Personal Identification number (Danish: CPR-nummer or
      personnummer) is a national identification number, which is part of the
      personal information stored in the Civil Registration System (Danish: Det
      Centrale Personregister).
      It is a ten-digit number with the format DDMMYYSSSS, where DDMMYY is
      the date of birth and SSSS is a sequence number. The last digit of the
      sequence number is odd for males and even for females.<br clear="none"/>
                <br clear="none"/>If the Danish Personal Identification number is unknown a validated
      replacement Danish Personal Identification number <b>SHALL</b> be used.<br clear="none"/>(CONF-DK: 1)<br clear="none"/>
            </desc>
            <classification type="datatypelevel"/>
            <example type="valid">
                <id extension="2512484916" root="1.2.208.176.1.2" assigningAuthorityName="CPR"/>
            </example>
            <element name="hl7:id" datatype="II" conformance="R" isMandatory="true" id="1.2.208.184.2.9.1">
                <desc language="en-US">The <i>id</i> element <b>SHALL</b> be present<br clear="none"/>
                    <br clear="none"/>
                </desc>
                <item label="CONF-DK: 2"/>
                <attribute name="extension" datatype="st" id="1.2.208.184.2.9.3">
                    <desc language="en-US">The value of the @<i>extension</i>&#160;<b>SHALL</b> be a valid Danish Personal Identification number (cpr-nummer)</desc>
                    <item label="CONF-DK: 3"/>
                </attribute>
                <attribute name="root" value="1.2.208.176.1.2" datatype="oid" id="1.2.208.184.2.9.2">
                    <desc language="en-US">The value of the @<i>root</i>&#160;<b>SHALL</b> be set to the associated OID for ‚ÄúDet Centrale Personregister‚Äù<br clear="none"/>
                        <br clear="none"/>
                    </desc>
                    <item label="CONF-DK: 4"/>
                </attribute>
                <attribute name="assigningAuthorityName" value="CPR" datatype="st" id="1.2.208.184.2.9.4">
                    <desc language="en-US">The value of the @<i>assignedAuthorityName</i>&#160;<b>SHALL</b> be set to ‚ÄúCPR‚Äù.<br clear="none"/>
                    </desc>
                    <item label="CONF-DK: 5"/>
                </attribute>
                <assert role="error" test="matches(@extension, '^[0-9]{10}$')">The value of the @extension SHALL be a valid Danish Personal Identification number (cpr-nummer)</assert>
            </element>
        </template>
        <!-- Danish Personal Name -->
        <template id="1.2.208.184.2.10.2" name="DKMedcomPersonalName" displayName="DK Medcom Personal Name" effectiveDate="2016-10-24T00:00:00" statusCode="active" isClosed="false" versionLabel="V1.1">
            <desc language="en-US">The <i>name</i> element is a set of reusable constraints that can be used for the
      patient or any other person. It requires a first (<i>given</i>) and last (<i>family</i>)
      name. One or more middle names can be inserted between the first and
      last name. If a patient or person has only one name part (e.g., patient
      with first name only) then place the name part in the best matching field.
      Use the appropriate <i>nullFlavor</i>, "Not Applicable" (NA), in the other field.<br clear="none"/>
            </desc>
            <classification type="datatypelevel"/>
            <example type="valid" caption="Name example">
                <name>
                    <given>Nancy</given>
                    <given>Ann</given>
                    <family>Berggren</family>
                </name>
            </example>
            <example type="valid" caption="Prefix example">
                <name>
                    <prefix>L√¶ge</prefix>
                    <given>Anders</given>
                    <family>Andersen</family>
                </name>
            </example>
            <element name="hl7:name" datatype="PN" id="1.2.208.184.2.9.5">
                <element name="hl7:family" datatype="ENXP" minimumMultiplicity="1" maximumMultiplicity="1" conformance="R" isMandatory="false" id="1.2.208.184.2.9.8">
                    <desc language="en-US">
                        <b>SHALL</b> contain exactly one [1..1] <i>family</i> element. In this profile the @<i>qualifier</i> is not used.<br clear="none"/>
                    </desc>
                    <item label="CONF-DK: 6"/>
                    <attribute name="nullFlavor" value="NA" datatype="cs" isOptional="true" id="1.2.208.184.2.9.9"/>
                </element>
                <element name="hl7:given" datatype="ENXP" minimumMultiplicity="1" maximumMultiplicity="*" conformance="R" isMandatory="false" id="1.2.208.184.2.9.6">
                    <desc language="en-US">
                        <b>SHALL</b> contain at least one [1..*] <i>given</i> element. In this profile the @<i>qualifier</i> is not used. The second occurrence of <i>given</i> (given[2]) if provided, <b>SHALL</b> include middle name or middle initial.<br clear="none"/>
                    </desc>
                    <item label="CONF-DK: 7"/>
                    <attribute name="nullFlavor" value="NA" datatype="cs" isOptional="true" id="1.2.208.184.2.9.7"/>
                </element>
                <element name="hl7:prefix" datatype="ENXP" minimumMultiplicity="0" maximumMultiplicity="1" id="1.2.208.184.2.9.10">
                    <desc language="en-US">
                        <b>MAY</b> contain one [0..1] <i>prefix</i> element, e.g. to include the title for a health professional. In this profile the @<i>qualifier</i> is not used.<br clear="none"/>
                    </desc>
                    <item label="CONF-DK: 8"/>
                </element>
            </element>
        </template>
        <template id="1.2.208.184.2.10.3" name="DKMedcomHospitalIdentification" displayName="DK Medcom Hospital Identification" effectiveDate="2016-10-24T00:00:00" statusCode="active" isClosed="false" versionLabel="V1.1">
            <desc language="en-US">The hospital sector is the responsibility of the five regions.
It is normally necessary to be referred by a general practitioner to a
hospital for medical examination and treatment, unless it is a question of
an accident or an acute illness. It will also normally be necessary to be
referred by a general practitioner for treatment by a specialist.<br clear="none"/>
                <br clear="none"/>The hospitals are responsible for specialised examinations, treatment and
care of somatic and mental illnesses which it would not be more
expedient to treat in the primary or social sector because of the need for
specialist knowledge, equipment or intensive care and monitoring. <br clear="none"/>
                <br clear="none"/>The Health Service Organisation Registry (Danish: Sundhedsv√¶senets
Organisationsregister - SOR) includes organisation and address
information for the public and private Hospitals in Denmark.<br clear="none"/>
            </desc>
            <classification type="datatypelevel"/>
            <example type="valid">
                <id extension="328151000016009" root="1.2.208.176.1.1" assigningAuthorityName="SOR"/>
            </example>
            <element name="hl7:id" datatype="II" isMandatory="true" id="1.2.208.184.2.9.11">
                <desc language="en-US">The <i>id</i>&#160;<b>SHALL</b> be present.</desc>
                <item label="CONF-DK: 19"/>
                <attribute name="extension" datatype="st" id="1.2.208.184.2.9.12">
                    <desc language="en-US">The value of the @<i>extension</i>&#160;<b>SHALL</b> be valid SOR code for a Hospital or
a Hospital ward.<br clear="none"/>
                    </desc>
                    <item label="CONF-DK: 20"/>
                </attribute>
                <attribute name="root" value="1.2.208.176.1.1" datatype="oid" id="1.2.208.184.2.9.13">
                    <desc language="en-US">The value of the @<i>root</i>&#160;<b>SHALL</b> be set to the associated OID for
Sundhedsv√¶senets Organisationsregister.<br clear="none"/>
                    </desc>
                    <item label="CONF-DK: 21"/>
                </attribute>
                <attribute name="assigningAuthorityName" value="SOR" datatype="st" id="1.2.208.184.2.9.14">
                    <desc language="en-US">The value of the @<i>assignedAuthorityName</i>&#160;<b>SHALL</b> be set to ‚ÄúSOR‚Äù<br clear="none"/>
                    </desc>
                    <item label="CONF-DK: 22"/>
                </attribute>
            </element>
        </template>
        <template id="1.2.208.184.2.10.4" name="DKMedcomConfidentialityCode" displayName="DK Medcom Confidentiality Code" effectiveDate="2016-10-24T00:00:00" statusCode="active" isClosed="false" versionLabel="V1.1">
            <desc language="en-US">
                <div>The <i>ClinicalDocument/confidentialityCode</i>&#160;<b>SHALL</b> be present. It specifies the confidentiality assigned to the document.</div>
                <div>The @<i>code</i> value <b>SHALL</b> be selected from the CDA Confidentiality Kind Value Set.</div>
                <div>The @<i>codeSystem</i> value <b>SHALL</b> be set to the associated OID for the HL7 Confidentiality</div>
            </desc>
            <classification type="datatypelevel"/>
            <item label="CONF-DK: 36"/>
            <example type="valid">
                <confidentialityCode code="N" codeSystem="2.16.840.1.113883.5.25"/>
            </example>
            <element name="hl7:confidentialityCode" datatype="CE" id="1.2.208.184.2.9.15">
                <attribute name="code" value="N" datatype="cs" id="1.2.208.184.2.9.16"/>
                <attribute name="codeSystem" value="2.16.840.1.113883.5.25" datatype="oid" id="1.2.208.184.2.9.17"/>
            </element>
        </template>
<!-- DK Timestamp -->
        <templateAssociation templateId="1.2.208.184.2.10.6" effectiveDate="2016-10-24T00:00:00"/>
        <template id="1.2.208.184.2.10.6" name="DKMedcomTimestampBody" displayName="DK Medcom Timestamp (Body)" effectiveDate="2016-10-24T00:00:00" statusCode="active" isClosed="false" versionLabel="V1.1">
            <desc language="en-US">Events occurring at a single point in time that are represented in the Clinical Document Header will in general be precise to the time. These point-in-time events are the time of creation of the document; the starting time of participation by an author, data enterer or the starting and ending time of an encounter.</desc>
            <classification type="datatypelevel"/>
            <example type="valid" caption="Timestamp example">
                <art:placeholder xmlns:art="urn:art-decor:example" value="20170215112652"/>
            </example>
            <attribute name="value" datatype="ts" id="1.2.208.184.2.9.18">
                <desc language="en-US">
                    <div>The representation of time <b>SHALL</b>&#160;use the format YYYYMMDDhhmmss¬±ZZzz<br/>
                        <br/>
                        <style>
            .mtr:nth-child(odd) {
            background-color: #ffeeee;
            }
          </style>
                        <div>
                            <table style="border-collapse: collapse; width: 100%">
                                <tbody>
                                    <tr class="mtr">
                                        <th style="border: 1px solid #ffeeee; text-align: left; font-size: 12px;background-color: #ffeeee">Token</th>
                                        <th style="border: 1px solid #ffeeee; text-align: left; font-size: 12px;background-color: #ffeeee">Description</th>
                                    </tr>
                                    <tr class="mtr">
                                        <td style="border: 1px solid #ffeeee">YYYY</td>
                                        <td style="border: 1px solid #ffeeee">The year of the event</td>
                                    </tr>
                                    <tr class="mtr">
                                        <td style="border: 1px solid #ffeeee">MM</td>
                                        <td style="border: 1px solid #ffeeee">The month in the year</td>
                                    </tr>
                                    <tr class="mtr">
                                        <td style="border: 1px solid #ffeeee">DD</td>
                                        <td style="border: 1px solid #ffeeee">The day in the month</td>
                                    </tr>
                                    <tr class="mtr">
                                        <td style="border: 1px solid #ffeeee">hh</td>
                                        <td style="border: 1px solid #ffeeee">The hour in the day</td>
                                    </tr>
                                    <tr class="mtr">
                                        <td style="border: 1px solid #ffeeee">mm</td>
                                        <td style="border: 1px solid #ffeeee">The minute of in the hour</td>
                                    </tr>
                                    <tr class="mtr">
                                        <td style="border: 1px solid #ffeeee">ss</td>
                                        <td style="border: 1px solid #ffeeee">The second in the minute</td>
                                    </tr>
                                    <tr class="mtr">
                                        <td style="border: 1px solid #ffeeee">¬±</td>
                                        <td style="border: 1px solid #ffeeee">Direction of the offset from UTC</td>
                                    </tr>
                                    <tr class="mtr">
                                        <td style="border: 1px solid #ffeeee">ZZ</td>
                                        <td style="border: 1px solid #ffeeee">Hours offset from UTC</td>
                                    </tr>
                                    <tr class="mtr">
                                        <td style="border: 1px solid #ffeeee">zz</td>
                                        <td style="border: 1px solid #ffeeee">Minutes offset in the hours offset from UTC</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </desc>
                <item label="CONF-DK: 34"/>
            </attribute>
            <assert role="error" test="matches(@value,'^[0-9]{14}[\+|\-][0-9]{4}$')">The representation of time SHALL use the format YYYYMMDDhhmmss¬±ZZzz</assert>
        </template>
        <template id="1.2.208.184.2.10.7" name="DKMedcomAddress" displayName="DK Medcom Address" effectiveDate="2016-10-24T00:00:00" statusCode="active" isClosed="false" versionLabel="V1.1">
            <desc language="en-US">
                <div>This template <b>SHOULD </b>be used for addresses in the <i>dan-DK</i> realm.</div>
            </desc>
            <classification type="datatypelevel"/>
            <example type="valid">
                <addr use="H">
                    <streetAddressLine>Hunderupvej 158</streetAddressLine>
                    <postalCode>5230</postalCode>
                    <city>Odense M</city>
                    <country>Danmark</country>
                </addr>
